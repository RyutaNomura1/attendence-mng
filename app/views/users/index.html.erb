<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<h2>ユーザー一覧</h2>
<% @users.each do |user| %>

<!--調べた時には:profile_imageと書かれていたが変更したら動いた-->
  <%= link_to user_path(user) do %> 
    <%= attachment_image_tag user, :profile_image, class: "profile_image", fallback: "no_image.jpg", size: '100x100' %>
    <%= user.username %>
  <% end %>
<% end %>